@using Microsoft.AspNetCore.Identity;
@model IAsyncEnumerable<IdentityUser>

@{
    ViewData["Title"] = "Управление пользователями";
}

<table class="table">
    <thead>
        <tr>
            <th>ID пользователя</th>
            <th>Логин</th>
            <th>Эл. почта</th>
            <th>Действия</th>
        </tr>
    </thead>
    <tbody>
        @await foreach (var user in Model)
        {
            @if(user.UserName != "admin")
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                    <td><a href="#">Удалить</a></td>
                </tr>
            }
        }

        <tr>
            <td>b53959ec-a912-4b74-9ff0-5foe501z0b98</td>
            <td>interviewer</td>
            <td>interviewcompany@yandex.ru</td>
            <td><a href="#">Удалить</a></td>
        </tr>
    </tbody>
</table>